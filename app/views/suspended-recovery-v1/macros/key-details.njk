{% macro keyDetails() %}
<section class="key-details-bar" role="contentinfo" aria-label="Key details">
  <dl class="key-details-bar__key-details">
    <div class="key-details-bar__top-block">
      <dt class="visually-hidden">Name:</dt>
      <dd class="key-details-bar__name">Jane Doe</dd>

      <dt class="visually-hidden">National Insurance number:</dt>
      <dd class="key-details-bar__nino">QQ 12 34 56 X</dd>

      <dt class="visually-hidden">Claim status:</dt>
      <dd class="key-details-bar__status">Complete</dd>
    </div>

    <div class="key-details-bar__bottom-block">
      <dt>Date of birth:</dt>
      <dd>4 June 1972</dd>

      <dt>Mobile:</dt>
      <dd>07771 900 900</dd>

      <dt>blah:</dt>
      <dd>dynamic template</dd>
    </div>
  </dl>
</section>
{% endmacro  %}